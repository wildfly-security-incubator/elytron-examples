#Change the signing algorithm
/subsystem=elytron-oidc-client/secure-deployment=oidc.war:write-attribute(name=request-object-signing-algorithm, value=RS256)

#add an encryption and content encryption algorithm
/subsystem=elytron-oidc-client/secure-deployment=oidc.war:write-attribute(name=request-object-encryption-algorithm, value=RSA-OAEP)
/subsystem=elytron-oidc-client/secure-deployment=oidc.war:write-attribute(name=request-object-content-encryption-algorithm, value=A256GCM)

#configure the keystore change the path to the actual path before running this script
/subsystem=elytron-oidc-client/secure-deployment=oidc.war:write-attribute(name=request-object-signing-keystore-file, value="path/to/jwt.keystore")
/subsystem=elytron-oidc-client/secure-deployment=oidc.war:write-attribute(name=request-object-signing-key-password, value=password)
/subsystem=elytron-oidc-client/secure-deployment=oidc.war:write-attribute(name=request-object-signing-keystore-password, value=password)
/subsystem=elytron-oidc-client/secure-deployment=oidc.war:write-attribute(name=request-object-signing-key-alias, value=rsaKey)
/subsystem=elytron-oidc-client/secure-deployment=oidc.war:write-attribute(name=request-object-signing-keystore-type, value=JKS)
reload




